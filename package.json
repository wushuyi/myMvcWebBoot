    "eqnull": false,
    "esnext": false,
    "evil": false,
    "expr": false,
    "funcscope": false,
    "globalstrict": false,
    "iterator": false,
    "lastsemic": false,
    "laxbreak": false,
    "laxcomma": false,
    "loopfunc": false,
    "moz": false,
    "multistr": false,
    "proto": false,
    "scripturl": false,
    "smarttabs": false,
    "shadow": false,
    "sub": false,
    "supernew": false,
    "validthis": false,
    "nomen": false,
    "onevar": false,
    "passfail": false,
    "white": true,
    "globals": {
      "browser": true,
      "couch": false,
      "devel": true,
      "dojo": false,
      "jquery": true,
      "mootools": false,
      "node": true,
      "nonstandard": false,
      "phantom": false,
      "prototypejs": false,
      "rhino": false,
      "worker": true,
      "wsh": false,
      "yui": false,
      "define": true,
      "requirejs": true,
      "require": true,
      "console": true
    }
